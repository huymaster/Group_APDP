@{
    ViewData["Title"] = "Users";
}

@model IEnumerable<User>

<h1>@ViewData["Title"]</h1>

<table class="table">
    <thead>
    <tr>
        <th scope="col">Id</th>
        <th scope="col">Name</th>
        <th scope="col">Email</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var user in Model)
    {
        <tr>
            <th scope="row">@user.Id</th>
            <td>@user.FullName</td>
            <td>@user.Email</td>
        </tr>
    }
    </tbody>
</table>